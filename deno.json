{
	"tasks": {
		"convert-bbcode-to-markdown": "deno run -A --env ./scripts/convert-bbcode-to-markdown.js",
		"convert-markdown-to-bbcode": "deno run -A --env ./scripts/convert-markdown-to-bbcode.js",
		"modrinth-binary-search": "deno run -A --env ./scripts/modrinth-binary-search.js",
		"transcribe-audio": "deno run -A --env ./scripts/transcribe-audio.js"
	},
	"imports": {
		"@bbob/core": "https://esm.sh/@bbob/core@^4.1.1",
		"@bbob/html": "https://esm.sh/@bbob/html@^4.1.1",
		"@bbob/preset-html5": "https://esm.sh/@bbob/preset-html5@^4.1.1",
		"@radashi-org/radashi": "jsr:@radashi-org/radashi@^12.3.0",
		"@std/path": "jsr:@std/path@^1.0.8",
		"markdown-ast": "npm:markdown-ast@^0.3.0",
		"turndown": "npm:turndown@^7.2.0"
	},
	"unstable": [
		"kv"
	],
	"compilerOptions": {
		"allowUnreachableCode": false,
		"allowUnusedLabels": false,
		"checkJs": true,
		"exactOptionalPropertyTypes": true,
		"jsx": "react-jsx",
		"jsxImportSource": "preact",
		"noErrorTruncation": true,
		"noFallthroughCasesInSwitch": false,
		"noImplicitAny": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noPropertyAccessFromIndexSignature": false,
		"noUncheckedIndexedAccess": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"strict": true,
		"lib": [
			"deno.ns",
			"dom",
			"dom.iterable",
			"dom.asynciterable",
			"deno.unstable"
		]
	},
	"nodeModulesDir": "none"
}